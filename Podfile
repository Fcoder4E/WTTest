#platform :ios, '10.0'

target 'WTTest' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WTTestest
  pod 'XCGLogger', '~> 5.0.1'
  pod 'OHHTTPStubs'
  pod 'OHHTTPStubs/Swift' # Adds the Swiftier API wrapper too
  
  target 'WTTestTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WTTestUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
